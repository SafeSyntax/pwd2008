div>h3
<div>
	<h3></h3>
</div>


.content>img+h2+p
<div class="content">
	<img src="" alt="">
	<h2></h2>
	<p></p>
</div>

.content>img[alt=photo]+h2+p
<div class="content">
	<img src="" alt="photo">
	<h2></h2>
	<p></p>
</div>


.content>h5+.in>img+h2+p
<div class="content">
	<h5></h5>
	<div class="in">
		<img src="" alt="">
		<h2></h2>
		<p></p>
	</div>
</div>



